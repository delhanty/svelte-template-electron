{
    "extends": "@tsconfig/svelte/tsconfig.json",
    "include": ["src/**/*"],
	"exclude": ["node_modules"],

	"compilerOptions": {
 		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"strict": true,

		"lib": ["dom", "dom.iterable", "esnext", "scripthost"]
	}
}
